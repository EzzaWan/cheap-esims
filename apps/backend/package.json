    "@types/dompurify": "^3.0.5",
    "@upstash/redis": "^1.35.7",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "geoip-lite": "^1.4.10",
    "handlebars": "^4.7.8",
    "isomorphic-dompurify": "^2.33.0",
