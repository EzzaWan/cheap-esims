{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "nest start",
    "dev": "nest start --watch --exec \"node -r ts-node/register/transpile-only src/main.ts\"",
